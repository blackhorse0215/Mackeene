<footer class="footer">
  <div class="footer__content">
    <div class="footer__content__form">
      <h5>Newsletter
      </h5>

      {%- form 'customer', id: 'footer-newsletter', class: 'Footer__Newsletter Form footer__content__form__container' -%}
        {%- if form.posted_successfully? -%}
          <p class="Form__Alert Alert Alert--success">{{ 'footer.newsletter.success' | t }}</p>
        {%- else -%}
          <div class="footer__content__form__label">
            <label for="newsletterInput" id="newsletterLabel">
              {{ 'footer.newsletter.input' | t }}
            </label>

            {%- if form.errors -%}
              <label for="newsletterInput" class="u-d-none" id="newsletterError">{{ form.errors.messages['email'] }}</label>
            {%- endif -%}
          </div>

          <div class="footer__content__form__input">
            <input type="hidden" name="contact[tags]" value="newsletter">
            <input type="email" id="newsletterInput" name="contact[email]" class="Form__Input" aria-label="{{ 'footer.newsletter.input' | t }}" placeholder="{{ 'footer.newsletter.input' | t }}" required>
            <button type="submit" class="footer__content__form__send" id="newsletterSend"></button>
          </div>
        {%- endif -%}
      {%- endform -%}

      <span class="footer__content__form__hint" id="newsletterHint">
        I’d like to sign up to the JACQUEMUS newsletter. My email won’t be shared with third parties.
      </span>
    </div>
    <div class="footer__content__right">
      <div class="footer__content__right__contact">
        <h5>Customer Care</h5>
        <div class="u-d-flex u-a-center">
