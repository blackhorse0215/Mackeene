        </path>
      </svg>
    </a>
  </div>
  <span class="header__back" id="headerBack"></span>
  <nav class="header__nav" id="headerNav">
    <ul class="header__nav__menu" id="headerMenu">
      {%- assign linklist = linklists[section.settings.main_menu] -%}
      {%- if linklist != empty -%}
        {%- for link in linklist.links -%}
          <li class="header__nav__list">
            <span id="{{ link.title | handle }}" class="header__nav__link">
              {{ link.title | escape }}
            </span>
            {%- if link.links != blank -%}
              <ul role="menu" class="header__nav__submenu" aria-hidden="true" aria-label="{{ link.title | handle }}">
                {%- for sub_link in link.links -%}
                  <li role="presentation" class="header__nav__sublist">
                    <ul role="menu" class="header__nav__submenu" aria-hidden="true" aria-label="col1">
                      {%- for sub_sub_link in sub_link.links -%}
                        <li role="presentation" class="header__nav__sublist">
                          <a href="{{ sub_sub_link.url }}" id="Neve_world_women"
                            class="header__nav__sublist__link">
                            <div class="-dark">
                              <span>{{ sub_sub_link.title | escape }}</span>
                              {%- comment -%} <span class="-count">116</span> {%- endcomment -%}
                            </div>
                            <span class="-light">
                              {{ sub_sub_link.title | escape }}
                            </span>
                          </a>
                        </li>
                      {%- endfor -%}      
                    </ul>
                  </li>
                {%- endfor -%}
              </ul>
            {%- endif -%}
          </li>
        {%- endfor -%}
      {%- endif -%}
    </ul>
    <div id="headerSubMenu" class="header__submenu"></div>
  </nav>
  <div class="header__secondarynav" id="headerSecondaryNav">
    <div class="header__search" id="headerSearch">
      <span class="header__search__link" id="panel-search-toggle">
        <span class="header__search__link__title">
          Search
        </span>
        <form class="search__form u-hidden-desktop" action="{{ routes.search_url }}" method="GET">
          <input class="search__form__input -mobileMenu" value="" type="text" name="q" autocomplete="off"
            placeholder="Search here...">
          <button class="search__form__submit" type="submit"></button>
        </form>
      </span>
      <dialog id="panel-search" class="panel origin--right -transition" style="width: 42vw; max-width: 90%;">
        <div class="panel__wrapper">
          <h3>
            Search
          </h3>
          <form class="search__form" action="{{ routes.search_url }}" method="GET">
            <input class="search__form__input" value="" type="text" name="q" autocomplete="off"
              placeholder="Search here...">
            <button class="search__form__submit" type="submit"></button>
          </form>
          <div class="panel__content">
            <div class="content-asset">
              <h5 class="header__search__title">Suggestions</h5>
              {%- assign linklist = linklists[section.settings.search_menu] -%}
              {%- if linklist != empty -%}
                {%- for link in linklist.links -%}
                  <a class="header__search__links" href="{{ link.url }}">{{ link.title | escape }}</a>
                {%- endfor -%}
              {%- endif -%}
            </div>
          </div>
          <div class="js-close-panel panel__close" data-panel-id="panel-search"><i class="icon icon-close_black"></i>
          </div>
        </div>
      </dialog>
    </div>
    <div class="header__account  navmobile__bottomAccount" id="headerAccount">
      <a class="header__account__link" href="{{ routes.account_url }}">
        Account
      </a>
      <dialog id="panel-account" class="panel origin--right -transition" style="width: 42vw; max-width: 90%;">

        <div class="panel__wrapper">
          <h3>Account</h3>
          <div class="js-close-panel panel__close" data-panel-id="panel-account"><i class="icon icon-close_black"></i>
          </div>
        </div>
      </dialog>
    </div>
    <div class="countrySwitcher__mobileNav  navmobile__bottomAccount" id="countrySwitcher__mobileNav">
      <div class="countrySwitcher__footer">
        <h6 class="countrySwitcher__footer__label">Country</h6>
        <div class="countrySwitcher__footer__action">
          <div class="dropdown country-selector langDropdown__wrapper"
            data-url="/on/demandware.store/Sites-Jacquemus-Site/en_DZ/Page-SetLocale" tabindex="0">
            <span class="btn dropdown-toggle langDropdown__selected" data-selector="dropdownCountrySelector"
              data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
              english
            </span>
            <div class="dropdown-menu dropdown-country-selector" aria-labelledby="dropdownCountrySelector">
              <a class="dropdown-item langDropdown__link" href="https://www.jacquemus.com/en_dz#" data-locale="fr_DZ"
                data-currencycode="USD">
                fran√ßais
              </a>
              <a class="dropdown-item langDropdown__link" href="https://www.jacquemus.com/en_dz#" data-locale="en_DZ"
                data-currencycode="USD">
                english
              </a>
            </div>
          </div>
          <span class="countrySwitcher__footer__separation"></span>
          <span data-selector="panelCountryswitcherToggle" class="countrySwitcher__footer__country">
            <i class="countrySwitcher__flag flag-icon flag-icon-dz"></i>
            Algeria (USD)
          </span>
          <span data-selector="country-panel-tmp" style="display:none;"></span>
        </div>
      </div>
    </div>
  </div>
  <div class="header__minicart">
    <div class="u-d-flex" id="panel-minicart-toggle">
      <span class="header__minicart__link u-hidden-mobile">
        Cart
      </span>
      <span class="header__minicart__amount" id="minicartAmount"></span>
    </div>
  </div>
  <span id="openNav" class="header__nav__open">
    <span class="header__nav__bar"></span>
    <span class="header__nav__bar"></span>
  </span>
</header>

{% schema %}
{
  "name": "Header",
  "class": "shopify-section--header",
  "settings": [
    {
      "type": "link_list",
      "id": "main_menu",
      "label": "Main Menu",
      "info": "This menu won't show dropdown items."
    },
    {
      "type": "link_list",
      "id": "search_menu",
      "label": "Search Menu",
      "info": "This menu won't show dropdown items."
    }
  ]
}
{% endschema %}